{
    "defaults": {
        "name": "Temp Storage Bucket",
        "region": "us-east-1",
        "versioning": false,
        "onDelete": "skip",
        "blockPublicAccess": true,
        "staticHosting": {
            "enabled": true,
            "indexDocument": "index.html",
            "errorDocument": "error.html"
        },
        "tags": {
            "app": "labrador",
            "env": "{{env}}"
        }
    },
    "buckets": [
        {
            "name": "{{env}}-{{project_name}}-assets",
            "onDelete": "delete",
            "staticHosting": {
                "enabled": false
            }
        }
    ]
}