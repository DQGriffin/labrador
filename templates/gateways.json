{
    "defaults": {
        "protocol": "http",
        "region": "us-east-1",
        "description": "API Gateway created by labrador",
        "tags": {
            "app": "labrador",
            "env": "{{env}}"
        }
    },
    "gateways": [
        {
            "name": "{{env}}-{{project_name}}-api",
            "integrations": [
                {
                    "type": "proxy",
                    "uri": "[[auth-func]]",
                    "payloadVersion": "2.0",
                    "integrationMethod": "POST",
                    "ref": "auth-func-int",
                    "target": {
                        "ref": "",
                        "external": {
                            "arn": "",
                            "dynamic": {
                                "name": "{{env}}-{{project_name}}-auth",
                                "region": "us-east-1",
                                "type": "lambda"
                            }
                        }
                    }
                }
            ],
            "routes": [
                {
                    "method": "GET",
                    "route": "/users",
                    "target": {
                        "ref": "auth-func-int"
                    }
                }
            ]
        }
    ]
}